{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "version",
    "runs"
  ],
  "properties": {
    "version": {
      "type": "string"
    },
    "runs": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "timestamp",
          "args",
          "exitCode",
          "steps"
        ],
        "properties": {
          "timestamp": {
            "type": "string"
          },
          "args": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "exitCode": {
            "type": "number"
          },
          "steps": {
            "type": "array",
            "items": {
              "type": "object",
              "required": [
                "line"
              ],
              "properties": {
                "line": {
                  "type": "string"
                },
                "input": {
                  "type": "string"
                },
                "isRegex": {
                  "type": "boolean"
                }
              }
            }
          }
        }
      }
    }
  }
}